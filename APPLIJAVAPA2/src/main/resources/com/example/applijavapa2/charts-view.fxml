      <BarChart fx:id="age" prefHeight="182.0" prefWidth="290.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <BarChart fx:id="revenu" layoutY="214.0" prefHeight="190.0" prefWidth="290.0">
         <xAxis>
            <CategoryAxis side="BOTTOM" />
